{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@dtos/*": [
        "./src/dtos/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@global/*": [
        "./src/global/*"
      ],
      "@infrastructure/*": [
        "./src/infrastructure/*"
      ],
      "@backend/*": [
        "./src/backend/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@stores/*": [
        "./src/stores/*"
      ],
    },
  },
}